Table ContactInfo {
  id int [PK]
  phone int
  address varchar
  email varchar
  website varchar
}

Table Business {
  id int [PK]
  contact int [ref: > ContactInfo.id]
  description varchar
  marketIds int[] [ref: <> FarmersMarket.id]
}

Table FarmersMarket {
  id int [PK]
  isSnapFriendly bool
  name varchar
  contact int [ref: > ContactInfo.id]
}

Table BusinessRepresentative {
  id int [PK]
  name varchar
  businessId int [ref: > Business.id]
}